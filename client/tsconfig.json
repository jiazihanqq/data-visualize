{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react",
    "lib": [
      "es5",
      "es2016",
      "es2017",
      "dom",
      "dom.iterable"
    ],
    "target": "es5",
    "module": "esnext",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
  "exclude": ["../node_modules"]
}
